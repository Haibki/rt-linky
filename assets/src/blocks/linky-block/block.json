{
    "$schema": "https://schemas.wp.org/trunk/block.json",
    "apiVersion": 3,
    "name": "rt-linky/profile",
    "version": "3.0.0",
    "title": "RT-Linky Profile",
    "category": "widgets",
    "description": "Display a RT-Linky profile on your page.",
    "supports": {
        "html": false,
        "align": ["wide", "full"]
    },
    "attributes": {
        "profileId": {
            "type": "number",
            "default": 0
        },
        "slug": {
            "type": "string",
            "default": ""
        }
    },
    "textdomain": "rt-linky",
    "editorScript": "file:./index.js",
    "editorStyle": "file:./index.css",
    "viewScript": "file:./view.js",
    "render": "file:./render.php"
}
